{
    "name": "cesargb/laravel-logs-rotate",
    "description": "Rotate files log with compression",
    "keywords": [
        "laravel",
        "logs",
        "rotate",
        "compression"
    ],
    "homepage": "https://github.com/cesargb/laravel-logs-rotate",
    "authors": [
	{
        "name": "Cesar Garcia",
        "email": "cesargb@gmail.com",
        "homepage": "https://github.com/cesargb",
        "role": "Developer"
	}
    ],
    "license": "MIT",
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.28 || ~5.6.0 || ~5.7.0",
        "illuminate/console": "~5.5.28 || ~5.6.0 || ~5.7.0",
        "illuminate/log": "~5.5.28 || ~5.6.0 || ~5.7.0",
        "illuminate/support": "~5.5.28 || ~5.6.0 || ~5.7.0",
        "monolog/monolog": "~1.20"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.2 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cesargb\\File\\Rotate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cesargb\\File\\Rotate\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cesargb\\File\\Rotate\\RotateServiceProvider"
            ]
        }
    }
}
