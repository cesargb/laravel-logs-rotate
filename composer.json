{
    "name": "cesargb/laravel-logs-rotate",
    "description": "Rotate files log with compression",
    "keywords": [
        "laravel",
        "logs",
        "rotate",
        "compression"
    ],
    "homepage": "https://github.com/cesargb/laravel-logs-rotate",
    "authors": [
	{
        "name": "Cesar Garcia",
        "email": "cesargb@gmail.com",
        "homepage": "https://github.com/cesargb",
        "role": "Developer"
	}
    ],
    "license": "MIT",
    "require": {
        "php" : "^7.2 | ^8.0",
        "illuminate/config": "^6.10|^7.0|^8.0",
        "illuminate/console": "^6.10|^7.0|^8.0",
        "illuminate/log": "^6.10|^7.0|^8.0",
        "illuminate/support": "^6.10|^7.0|^8.0",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.10|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cesargb\\File\\Rotate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cesargb\\File\\Rotate\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cesargb\\File\\Rotate\\RotateServiceProvider"
            ]
        }
    }
}
